{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CountryDataFactroy"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Database_1_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Database_1_sql'"
		},
		"Database_2_sql_copydatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Database_2_sql_copydatabase'"
		},
		"Database_1_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverpoc728.database.windows.net"
		},
		"Database_1_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "pocdatabase56"
		},
		"Database_1_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rajanishku"
		},
		"Database_2_sql_copydatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverpoc728.database.windows.net"
		},
		"Database_2_sql_copydatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Copydatabase"
		},
		"Database_2_sql_copydatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rajanishku"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CountryDataFactroy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Database_1_sql_password": {
						"value": "[parameters('Database_1_sql_password')]"
					},
					"Database_2_sql_copydatabase_password": {
						"value": "[parameters('Database_2_sql_copydatabase_password')]"
					},
					"Database_1_sql_properties_typeProperties_server": {
						"value": "[parameters('Database_1_sql_properties_typeProperties_server')]"
					},
					"Database_1_sql_properties_typeProperties_database": {
						"value": "[parameters('Database_1_sql_properties_typeProperties_database')]"
					},
					"Database_1_sql_properties_typeProperties_userName": {
						"value": "[parameters('Database_1_sql_properties_typeProperties_userName')]"
					},
					"Database_2_sql_copydatabase_properties_typeProperties_server": {
						"value": "[parameters('Database_2_sql_copydatabase_properties_typeProperties_server')]"
					},
					"Database_2_sql_copydatabase_properties_typeProperties_database": {
						"value": "[parameters('Database_2_sql_copydatabase_properties_typeProperties_database')]"
					},
					"Database_2_sql_copydatabase_properties_typeProperties_userName": {
						"value": "[parameters('Database_2_sql_copydatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}